{
  "name": "mind_maps_project",
  "version": "1.0.0",
  "description": "my personal coggle_like mind map app",
  "main": ".eslintrc.js",
  "scripts": {
    "doc": "node_modules/jsdoc/jsdoc.js ./node_classes_js/  ./Engine_class_js/ ./path_classes_js",
