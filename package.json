  "license": "ISC",
  "bugs": {
    "url": "https://github.com/7ganam/mind_map/issues"
  },
  "homepage": "https://github.com/7ganam/mind_map#readme",
  "dependencies": {
    "jsdoc": "^3.6.5",
    "onchange": "^7.0.2"
  },
  "devDependencies": {
    "better-docs": "^2.3.0",
    "jsdoc-mermaid": "^1.0.0"
  }
}
